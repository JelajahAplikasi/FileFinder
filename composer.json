{
    "name": "muharihar/file-finder",
    "description": "Laravel Local File Finder Package",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Muhammad Hari Suharto",
            "email": "hi.muhammad.hari@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Muharihar\\FileFinder\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Muharihar\\FileFinder\\ServiceProviders\\FileFinderServiceProvider"
            ],
            "aliases": {
                "FileFinder": "Muharihar\\FileFinder\\Facades\\FileFinder"
            }
        }
    },
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": ">=5.6"
    },
    "require-dev": {
        "laravel/dusk": "^4.0",
        "phpunit/phpunit": "^7.0"
    }
}
